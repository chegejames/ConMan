<h1>Listing service_procurements</h1>

<table class="table table-striped table-condensed table-hover">
  <tr>
    <th>Service</th>
    <th>Phase</th>
    <th>Provider</th>
    <th>Date</th>
    <th>Cost</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @service_procurements.each do |service_procurement| %>
  <tr>
    <td><%= service_procurement.service.name %></td>
    <td><%= service_procurement.phase.name %></td>
    <td><%= service_procurement.provider %></td>
    <td><%= service_procurement.date %></td>
    <td><%= service_procurement.cost %></td>
    <td><%= link_to 'Show', [@project, service_procurement] %></td>
    <td><%= link_to 'Edit', edit_project_service_procurement_path(@project, service_procurement) %></td>
    <td><%= link_to 'Destroy', [@project, service_procurement], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Service procurement', new_project_service_procurement_path(@project) %>
